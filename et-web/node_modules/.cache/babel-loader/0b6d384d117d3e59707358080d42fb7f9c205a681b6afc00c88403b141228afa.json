{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { connect, sendMessage, subscribePublic } from \"@/services/WebSocketService\";\nexport default {\n  data() {\n    return {\n      username: '',\n      messages: [],\n      messageContent: '',\n      websocket: null\n    };\n  },\n  mounted() {\n    connect();\n  },\n  methods: {\n    connect() {\n      let socketUrl = \"ws://localhost:8080/api/websocket/conversation/1\";\n      websocket = new WebSocket(socketUrl);\n      websocket.onmessage = function (message) {\n        console.log(message.data);\n      };\n    },\n    onMessageReceived(message) {\n      this.messages.push(message);\n    },\n    sendMessage() {\n      if (this.messageContent.trim()) {\n        var message = this.messageContent;\n        websocket.send(message); // websocket 发送消息\n        this.messages.push(message); // 消息历史\n        this.messageContent = ''; // 清空输入框\n      }\n    }\n  }\n};","map":{"version":3,"names":["connect","sendMessage","subscribePublic","data","username","messages","messageContent","websocket","mounted","methods","socketUrl","WebSocket","onmessage","message","console","log","onMessageReceived","push","trim","send"],"sources":["D:\\000Projects\\JAVA\\emp-train\\et-web\\src\\components\\WebSocketTest.vue"],"sourcesContent":["<template>\n  <div>\n    <div v-for=\"message in messages\" :key=\"message.sender\">\n      <p><strong>{{ message.sender }}:</strong> {{ message.content }}</p>\n    </div>\n\n    <input v-model=\"messageContent\" placeholder=\"Type a message...\">\n    <button @click=\"sendMessage\">send</button>\n  </div>\n</template>\n\n<script>\nimport { connect, sendMessage, subscribePublic } from \"@/services/WebSocketService\";\n\nexport default {\n  data() {\n    return {\n      username: '',\n      messages: [],\n      messageContent: '',\n      websocket: null\n    };\n  },\n  mounted() {\n    connect();\n  },\n  methods: {\n    connect() {\n      let socketUrl = \"ws://localhost:8080/api/websocket/conversation/1\"\n      websocket = new WebSocket(socketUrl);\n      websocket.onmessage = function (message) {\n        console.log(message.data);\n      }\n    },\n    onMessageReceived(message) {\n      this.messages.push(message);\n    },\n    sendMessage() {\n      if (this.messageContent.trim()) {\n        var message = this.messageContent;\n        websocket.send(message); // websocket 发送消息\n        this.messages.push(message); // 消息历史\n        this.messageContent = ''; // 清空输入框\n      }\n    }\n  }\n};\n</script>\n"],"mappings":";AAYA,SAASA,OAAO,EAAEC,WAAW,EAAEC,eAAc,QAAS,6BAA6B;AAEnF,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,cAAc,EAAE,EAAE;MAClBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRR,OAAO,CAAC,CAAC;EACX,CAAC;EACDS,OAAO,EAAE;IACPT,OAAOA,CAAA,EAAG;MACR,IAAIU,SAAQ,GAAI,kDAAiD;MACjEH,SAAQ,GAAI,IAAII,SAAS,CAACD,SAAS,CAAC;MACpCH,SAAS,CAACK,SAAQ,GAAI,UAAUC,OAAO,EAAE;QACvCC,OAAO,CAACC,GAAG,CAACF,OAAO,CAACV,IAAI,CAAC;MAC3B;IACF,CAAC;IACDa,iBAAiBA,CAACH,OAAO,EAAE;MACzB,IAAI,CAACR,QAAQ,CAACY,IAAI,CAACJ,OAAO,CAAC;IAC7B,CAAC;IACDZ,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACK,cAAc,CAACY,IAAI,CAAC,CAAC,EAAE;QAC9B,IAAIL,OAAM,GAAI,IAAI,CAACP,cAAc;QACjCC,SAAS,CAACY,IAAI,CAACN,OAAO,CAAC,EAAE;QACzB,IAAI,CAACR,QAAQ,CAACY,IAAI,CAACJ,OAAO,CAAC,EAAE;QAC7B,IAAI,CAACP,cAAa,GAAI,EAAE,EAAE;MAC5B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}